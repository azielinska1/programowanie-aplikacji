# Gemini API key (paste your own key here)
GEMINI_API_KEY=

# Gemini model name (examples: gemini-2.5-flash, gemini-2.0-flash, gemini-1.5-flash)
GEMINI_MODEL=gemini-2.5-flash

# App settings
APP_HOST=127.0.0.1
APP_PORT=8000

# SQLite database file
SQLITE_PATH=./app.db

# Optional: return tool trace in /api/chat
DEBUG_TOOL_TRACE=false
